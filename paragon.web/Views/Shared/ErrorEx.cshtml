@model System.Exception

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error @Model.Source</title>
</head>
<body>
    <h2>
        Sorry, an error occurred while processing your request.
    </h2>
    <p>@Model.Message</p>
    <blockquote>@Model.StackTrace</blockquote>
</body>
</html>